entity Bike {
    name String,
	image ImageBlob,
}

entity Travel{
	name String,
}

entity Location{
	name String,
    latitude Double,
   	longitude Double
}

relationship OneToMany{
    Travel{location} to Location
}

relationship ManyToOne{
	// Location{travel} to Travel,
	Bike{user} to User,
    Travel{user} to User,
    Travel{bike} to Bike
}
